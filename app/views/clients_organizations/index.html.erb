<p id="notice"><%= notice %></p>

<h1>Clients Organizations</h1>

<table>
  <thead>
    <tr>
      <th>Client</th>
      <th>Organization</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @clients_organizations.each do |clients_organization| %>
      <tr>
        <td><%= clients_organization.client_id %></td>
        <td><%= clients_organization.organization_id %></td>
        <td><%= link_to 'Show', clients_organization %></td>
        <td><%= link_to 'Edit', edit_clients_organization_path(clients_organization) %></td>
        <td><%= link_to 'Destroy', clients_organization, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Clients Organization', new_clients_organization_path %>
